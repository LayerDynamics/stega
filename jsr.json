{
  "name": "@layerdynamics/stega",
  "version": "1.0.0-beta.1",
  "exports": "./src/mod.ts",
  "publish": {
    "include": [
      "LICENSE",
      "README.md",
      "src/mod.ts",
      "src/core/**/*.ts",
      "src/compiler/**/*.ts",
