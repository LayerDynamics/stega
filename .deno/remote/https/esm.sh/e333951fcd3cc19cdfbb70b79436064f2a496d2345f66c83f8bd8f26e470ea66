var d=/^[\w+.-]+:\/\//,x=/^([\w+.-]+:)\/\/([^@/#?]*@)?([^:/#?]*)(:\d+)?(\/[^#?]*)?(\?[^#]*)?(#.*)?/,U=/^file:(?:\/\/((?![a-z]:)[^/#?]*)?)?(\/?[^#?]*)(\?[^#]*)?(#.*)?/i;function W(e){return d.test(e)}function g(e){return e.startsWith("//")}function p(e){return e.startsWith("/")}function R(e){return e.startsWith("file:")}function f(e){return/^[.?#]/.test(e)}function l(e){let s=x.exec(e);return m(s[1],s[2]||"",s[3],s[4]||"",s[5]||"/",s[6]||"",s[7]||"")}function q(e){let s=U.exec(e),t=s[2];return m("file:","",s[1]||"","",p(t)?t:"/"+t,s[3]||"",s[4]||"")}function m(e,s,t,n,c,r,i){return{scheme:e,user:s,host:t,port:n,path:c,query:r,hash:i,type:7}}function u(e){if(g(e)){let t=l("http:"+e);return t.scheme="",t.type=6,t}if(p(e)){let t=l("http://foo.com"+e);return t.scheme="",t.host="",t.type=5,t}if(R(e))return q(e);if(W(e))return l(e);let s=l("http://foo.com/"+e);return s.scheme="",s.host="",s.type=e?e.startsWith("?")?3:e.startsWith("#")?2:4:1,s}function v(e){if(e.endsWith("/.."))return e;let s=e.lastIndexOf("/");return e.slice(0,s+1)}function w(e,s){y(s,s.type),e.path==="/"?e.path=s.path:e.path=v(s.path)+e.path}function y(e,s){let t=s<=4,n=e.path.split("/"),c=1,r=0,i=!1;for(let o=1;o<n.length;o++){let a=n[o];if(!a){i=!0;continue}if(i=!1,a!=="."){if(a===".."){r?(i=!0,r--,c--):t&&(n[c++]=a);continue}n[c++]=a,r++}}let h="";for(let o=1;o<c;o++)h+="/"+n[o];(!h||i&&!h.endsWith("/.."))&&(h+="/"),e.path=h}function P(e,s){if(!e&&!s)return"";let t=u(e),n=t.type;if(s&&n!==7){let r=u(s),i=r.type;switch(n){case 1:t.hash=r.hash;case 2:t.query=r.query;case 3:case 4:w(t,r);case 5:t.user=r.user,t.host=r.host,t.port=r.port;case 6:t.scheme=r.scheme}i>n&&(n=i)}y(t,n);let c=t.query+t.hash;switch(n){case 2:case 3:return c;case 4:{let r=t.path.slice(1);return r?f(s||e)&&!f(r)?"./"+r+c:r+c:c||"."}case 5:return t.path+c;default:return t.scheme+"//"+t.user+t.host+t.port+t.path+c}}export{P as default};
